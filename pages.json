{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom" // 隐藏系统导航栏
			}

		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "外卖",
				"navigationStyle": "custom" // 隐藏系统导航栏
			}
		}, {
			"path": "pages/search/search",
			"style": {
				"navigationBarTitleText": "搜索",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom" // 隐藏系统导航栏
			}

		}, {
			"path": "pages/order/order",
			"style": {
				"navigationBarTitleText": "订单",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom" // 隐藏系统导航栏
			}

		}, {
			"path": "pages/mone/mone",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false,
				"navigationStyle": "custom" // 隐藏系统导航栏
			}

		}

	    ,{
            "path" : "components/heardNavbar/heardNavbar",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"tabBar": {
		"color": "#515151",
		"fontSize": "28rpx",
		"spacing": "0rpx",
		"selectedColor": "#02a774",
		"list": [{
			"iconPath": "static/image/mobile-phone-h.png",
			"selectedIconPath": "static/image/mobile-phone.png",
			"text": "外卖",
			"pagePath": "pages/index/index"
		}, {
			"iconPath": "static/image/search-h.png",
			"selectedIconPath": "static/image/search.png",
			"text": "搜索",
			"pagePath": "pages/search/search"
		}, {
			"iconPath": "static/image/order-h.png",
			"selectedIconPath": "static/image/order.png",
			"text": "订单",
			"pagePath": "pages/order/order"
		}, {
			"iconPath": "static/image/wode-h.png",
			"selectedIconPath": "static/image/wode.png",
			"text": "我的",
			"pagePath": "pages/mone/mone"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
